generator client {
  provider = "prisma-client-js"
}

datasource sqlite {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Song {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
  title     String
  artist    String
  setId     Int?
  Set       Set?     @relation(fields: [setId], references: [id])
}

model Set {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  setName   String   @unique
  password  String
  songs     Song[]
}
